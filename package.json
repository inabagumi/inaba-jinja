  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "bugs": {
    "email": "contact@haneru.dev",
    "url": "https://github.com/inabagumi/inaba-jinja/issues"
  },
  "dependencies": {
    "@docusaurus/lqip-loader": "^2.0.0-alpha.63",
    "@mdx-js/loader": "^1.6.16",
    "@next/mdx": "^9.5.3",
    "@next/plugin-google-analytics": "^9.5.3",
    "@next/plugin-sentry": "^9.5.3",
    "@sentry/webpack-plugin": "^1.12.1",
    "@svgr/webpack": "^5.4.0",
    "clsx": "^1.1.1",
    "content-security-policy-builder": "^2.1.0",
    "contentful": "^7.14.6",
    "file-loader": "^6.1.0",
    "next": "^9.5.3",
    "next-offline": "^5.0.2",
    "next-seo": "^4.7.3",
    "nprogress": "^1.0.0-1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sharp": "^0.26.0",
    "url-loader": "^4.1.0"
  },
  "description": "The source code for the Inaba Jinja.",
  "devDependencies": {
    "@inabagumi/eslint-config-react": "^6.0.1",
    "@inabagumi/prettier-config": "^1.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@types/gtag.js": "^0.0.3",
    "@types/jest": "^26.0.13",
    "@types/mdx-js__react": "^1.5.2",
    "@types/node": "^12.12.47",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.49",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@types/webpack-env": "^1.15.2",
    "conventional-github-releaser": "3.1.5",
    "eslint": "^7.8.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "12.x"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ],
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "root": true
  },
  "homepage": "https://inaba-jinja.com",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/src/__mocks__/svgMock.tsx"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "/.next/",
      "/node_modules/"
    ],
    "testURL": "https://inaba-jinja.test"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,ts,tsx,yml}": "yarn format-check",
    "*.{js,ts,tsx}": "yarn lint"
  },
  "main": "n/a",
  "name": "inaba-jinja",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/inabagumi/inaba-jinja.git"
  },
  "resolutions": {
    "@sentry/browser": "^5.23.0",
    "@sentry/node": "^5.23.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier --write .",
    "format-check": "prettier -c .",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "release": "standard-version",
    "start": "next start",
    "test": "jest"
  },
  "version": "1.11.5"
}
