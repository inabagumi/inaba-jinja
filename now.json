{
  "build": {
    "env": {
      "CONTENTFUL_ACCESS_TOKEN": "@inaba-jinja-contentful-access-token",
      "CONTENTFUL_SPACE_ID": "@inaba-jinja-contentful-space-id",
      "GA_TRACKING_ID": "@inaba-jinja-ga-tracking-id",
      "SENTRY_DSN": "@inaba-jinja-sentry-dsn"
    }
  },
  "github": {
    "autoAlias": false,
    "silent": true
  },
  "name": "inaba-jinja",
  "regions": ["hnd1", "sfo1"],
  "routes": [
    {
      "continue": true,
      "headers": {
        "content-type": "application/manifest+json"
      },
      "src": "/.+\\.webmanifest"
    },
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=600, public, s-maxage=120, stale-if-error=600, stale-while-revalidate=300",
        "content-security-policy": "base-uri 'none'; connect-src 'self' https:; default-src 'self'; form-action 'none'; frame-ancestors 'none'; img-src 'self' https: data:; manifest-src 'self'; object-src 'none'; script-src 'self' 'unsafe-inline' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com; style-src 'unsafe-inline'; worker-src 'self'",
        "referrer-policy": "same-origin, strict-origin-when-cross-origin"
      },
      "src": "/.*"
    },
    {
      "continue": true,
      "headers": {
        "cache-control": "immutable, public, max-age=31536000"
      },
      "src": "/_next/.+"
    },
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=0",
        "service-worker-allowed": "/"
      },
      "src": "/service-worker\\.js"
    }
  ],
  "scope": "inabagumi",
  "version": 2
}
