{
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "esbenp.prettier-vscode",
        "unifiedjs.vscode-mdx"
      ],
      "settings": {}
    }
  },
  "forwardPorts": [3000],
  "hostRequirements": {
    "cpus": 4,
    "memory": "8gb",
    "storage": "32gb"
  },
  "image": "mcr.microsoft.com/devcontainers/typescript-node:0-18-buster",
  "name": "Inaba Jinja",
  "postCreateCommand": "sudo corepack enable && pnpm install",
  "waitFor": "postCreateCommand"
}
