{
  "author": "Haneru Developers",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.60",
    "@docusaurus/plugin-ideal-image": "^2.0.0-alpha.60",
    "@docusaurus/preset-classic": "^2.0.0-alpha.60",
    "classnames": "^2.2.6",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "description": "We are Haneru Developers!",
  "devDependencies": {
    "@inabagumi/eslint-config-react": "^5.3.1",
    "@inabagumi/prettier-config": "^1.1.2",
    "eslint": "^7.5.0",
    "prettier": "^2.0.5",
    "typescript": "~3.9.7"
  },
  "engines": {
    "node": "12.x"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "license": "MIT",
  "name": "@inabagumi/website",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "swizzle": "docusaurus swizzle",
    "now-build": "${npm_execpath} run build"
  },
  "version": "2.0.0"
