{
  "author": "Haneru Developers",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.66",
    "@docusaurus/plugin-ideal-image": "^2.0.0-alpha.66",
    "@docusaurus/preset-classic": "^2.0.0-alpha.66",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "description": "We are Haneru Developers!",
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.66",
    "@inabagumi/eslint-config-react": "^6.0.1",
    "@inabagumi/prettier-config": "^1.1.2",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.53",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "eslint": "^7.11.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12.x"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ],
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-unsafe-member-access": "off"
    }
  },
  "license": "MIT",
  "name": "@inabagumi/website",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ignore-path .gitignore .",
    "swizzle": "docusaurus swizzle",
    "now-build": "${npm_execpath} run build"
  },
  "version": "2.0.0"
}
