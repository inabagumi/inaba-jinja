{
  "dependencies": {},
  "description": "We are Haneru Developers!",
  "devDependencies": {
    "@inabagumi/prettier-config": "^1.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vuepress/plugin-back-to-top": "^1.0.2",
    "@vuepress/plugin-google-analytics": "^1.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "vuepress": "^1.0.2",
    "vuepress-plugin-sitemap": "^2.1.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ]
  },
  "license": "MIT",
  "name": "@inabagumi/website",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "vuepress build ./docs/",
    "dev": "vuepress dev ./docs/",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{json,md}\"",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .gitignore docs/.vuepress",
    "now-build": "${npm_execpath} run build"
  }
}
