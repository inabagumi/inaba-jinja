{
  "author": "Haneru Developers",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "description": "Feel the life of Neru-chan!",
  "devDependencies": {
    "@inabagumi/eslint-config-react": "^4.3.0",
    "@inabagumi/prettier-config": "^1.1.0",
    "@types/node": "^12.11.2",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "prettier": "^1.18.2",
    "react-scripts": "^3.2.0",
    "typescript": "^3.6.4"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ]
  },
  "homepage": "https://hanerubeat.app/",
  "name": "@inabagumi/hanerubeat",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{html,js,json,ts,tsx}\"",
    "format-check": "prettier --ignore-path .gitignore --check \"**/*.{html,js,json,ts,tsx}\"",
    "now-build": "${npm_execpath} run build"
  },
  "version": "2.0.0"
}
