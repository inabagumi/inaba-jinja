{
  "author": "Haneru Developers",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "description": "Feel the life of Neru-chan!",
  "devDependencies": {
    "@inabagumi/eslint-config-react": "^5.1.1",
    "@inabagumi/prettier-config": "^1.1.2",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "prettier": "^2.0.2",
    "react-scripts": "^3.4.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "12.x"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ]
  },
  "homepage": "https://hanerubeat.app/",
  "name": "@inabagumi/hanerubeat",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{html,js,json,ts,tsx}\"",
    "format-check": "prettier --ignore-path .gitignore --check \"**/*.{html,js,json,ts,tsx}\"",
    "now-build": "${npm_execpath} run build"
  },
  "version": "2.0.0"
}
