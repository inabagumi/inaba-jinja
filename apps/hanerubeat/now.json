{
  "build": {
    "env": {
      "REACT_APP_DESCRIPTION": "「Haneru Beat!」は因幡はねるさんの鼓動を感じるためのアプリです。因幡はねるさんの鼓動を感じて、因幡はねるさんに包まれて眠りましょう。",
      "REACT_APP_TITLE": "Haneru Beat!",
      "REACT_APP_URL": "https://hanerubeat.app"
    }
  },
  "builds": [
    {
      "config": {
        "distDir": "build"
      },
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "github": {
    "silent": true
  },
  "name": "hanerubeat",
  "routes": [
    {
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "src": "/static/(?:js|media)/.+"
    },
    {
      "headers": {
        "cache-control": "max-age=600, s-maxage=60"
      },
      "src": "/.*"
    }
  ],
  "regions": ["hnd1"],
  "scope": "inabagumi",
  "version": 2
}
