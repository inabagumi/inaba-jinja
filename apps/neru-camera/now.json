{
  "alias": ["neru.camera"],
  "builds": [
    {
      "config": {
        "maxLambdaSize": "50mb"
      },
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "github": {
    "autoAlias": false,
    "silent": true
  },
  "name": "neru-camera",
  "regions": ["hnd1"],
  "routes": [
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=600, s-maxage=60"
      },
      "src": ".*"
    },
    {
      "handle": "filesystem"
    },
    {
      "dest": "/_error",
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": ".*"
    }
  ],
  "scope": "inabagumi",
  "version": 2
}
