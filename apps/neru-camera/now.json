{
  "alias": ["neru.camera"],
  "build": {
    "env": {
      "NERU_CAMERA_BASE_URL": "https://neru.camera",
      "NERU_CAMERA_DESCRIPTION": "ねるカメラは有閑喫茶 あにまーれに所属しているバーチャルYouTuberの因幡はねるさん (ねるちゃん) と一緒に写真が撮れるアプリです。ねるちゃんとの幸せなひとときをあなたに。",
      "NERU_CAMERA_TITLE": "ねるカメラ"
    }
  },
  "builds": [
    {
      "config": {
        "maxLambdaSize": "50mb"
      },
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "github": {
    "autoAlias": false,
    "silent": true
  },
  "name": "neru-camera",
  "regions": ["hnd1"],
  "routes": [
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=600, s-maxage=60"
      },
      "src": ".*"
    },
    {
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": "/service-worker\\.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "dest": "/_error",
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": ".*"
    }
  ],
  "scope": "inabagumi",
  "version": 2
}
