{
  "alias": ["neru.camera"],
  "build": {
    "env": {
      "CONTENTFUL_ACCESS_TOKEN": "@neru-camera-contentful-access-token",
      "CONTENTFUL_SPACE_ID": "@neru-camera-contentful-space-id",
      "NERU_CAMERA_BASE_URL": "https://neru.camera",
      "NERU_CAMERA_DESCRIPTION": "ねるカメラは有閑喫茶 あにまーれに所属しているバーチャルYouTuberの因幡はねるさん (ねるちゃん) と一緒に写真が撮れるアプリです。ねるちゃんとの幸せなひとときをあなたに。",
      "NERU_CAMERA_TITLE": "ねるカメラ"
    }
  },
  "github": {
    "autoAlias": false,
    "silent": true
  },
  "name": "neru-camera",
  "regions": ["hnd1"],
  "routes": [
    {
      "continue": true,
      "headers": {
        "cache-control": "max-age=600, s-maxage=60"
      },
      "src": ".*"
    },
    {
      "headers": {
        "cache-control": "max-age=0"
      },
      "src": "/service-worker\\.js"
    }
  ],
  "scope": "inabagumi",
  "version": 2
}
