{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "videos",
      "version": 1,
      "urls": [
        "https://video.twimg.com/**"
      ],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "3d",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
