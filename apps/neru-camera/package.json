{
  "author": "Haneru Developers",
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "dependencies": {
    "pinch-zoom-element": "^1.1.1",
    "pixi.js": "^5.1.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1",
    "workbox-window": "^4.3.1"
  },
  "description": "Neru Camera is an app to take a photos with Neru-chan!",
  "devDependencies": {
    "@inabagumi/eslint-config-vue": "^4.1.1",
    "@inabagumi/prettier-config": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/workbox-window": "^4.3.2",
    "@vue/cli-plugin-babel": "^4.0.0-rc.0",
    "@vue/cli-plugin-eslint": "^4.0.0-rc.0",
    "@vue/cli-plugin-pwa": "^4.0.0-rc.0",
    "@vue/cli-plugin-typescript": "^4.0.0-rc.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0-rc.0",
    "@vue/cli-service": "^4.0.0-rc.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "eslint": "^6.2.2",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "~3.6.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/vue"
    ],
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "testURL": "https://neru-camera.test/",
    "transform": {
      "\\.ts$": "ts-jest",
      "\\.vue$": "vue-jest"
    }
  },
  "license": "MIT",
  "name": "neru-camera",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{html,json,md,yml}\"",
    "lint": "vue-cli-service lint --ignore-path .gitignore",
    "now-build": "${npm_execpath} run build",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit"
  },
  "version": "4.1.0"
}
